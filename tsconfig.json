{
  "extends": "@quasar/app-vite/tsconfig-preset",
  "compilerOptions": {
    "baseUrl": "./",
    "types": [
      "vite-plugin-pages/client",
      "vite-plugin-vue-layouts/client",
      "node"
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "src/*": [
        "./src/*"
      ],
      "components/*": [
        "./src/components/*"
      ],
      "composables/*": [
        "./src/composables/*"
      ],
      "layouts/*": [
        "./src/layouts/*"
      ],
      "pages/*": [
        "./src/pages/*"
      ],
      "stores/*": [
        "./src/stores/*"
      ],
      "types/*": [
        "./src/types/*"
      ],
      "utils/*": [
        "./src/utils/*"
      ]
    }
  },
  "exclude": [
    "./dist",
    "./.quasar",
    "./node_modules",
    "./src-capacitor",
    "./src-cordova",
    "./quasar.config.*.temporary.compiled*"
  ],
  "include": [
    "./src"
  ]
}